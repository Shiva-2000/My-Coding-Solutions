import math
import os
import random
import re
import sys

# Complete the miniMaxSum function below.
def miniMaxSum(arr):
    arr.sort();s1=s2=0
    for i in range(4):
        s1+=arr[i]
    arr.reverse()
    for i in range(4):
        s2+=arr[i]
    print(s1,s2)
if __name__ == '__main__':
    arr = list(map(int, input().rstrip().split()))

    miniMaxSum(arr)
